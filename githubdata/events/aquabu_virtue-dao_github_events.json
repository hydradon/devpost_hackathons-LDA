                    "gists_url": "https://api.github.com/users/aquabu/gists{/gist_id}",
                    "starred_url": "https://api.github.com/users/aquabu/starred{/owner}{/repo}",
                    "subscriptions_url": "https://api.github.com/users/aquabu/subscriptions",
                    "organizations_url": "https://api.github.com/users/aquabu/orgs",
                    "repos_url": "https://api.github.com/users/aquabu/repos",
                    "events_url": "https://api.github.com/users/aquabu/events{/privacy}",
                    "received_events_url": "https://api.github.com/users/aquabu/received_events",
                    "type": "User",
