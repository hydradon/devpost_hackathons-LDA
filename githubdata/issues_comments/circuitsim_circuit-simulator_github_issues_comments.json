            "organizations_url": "https://api.github.com/users/ThomWright/orgs",
            "repos_url": "https://api.github.com/users/ThomWright/repos",
            "events_url": "https://api.github.com/users/ThomWright/events{/privacy}",
            "received_events_url": "https://api.github.com/users/ThomWright/received_events",
