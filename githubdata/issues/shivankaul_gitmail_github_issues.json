        "title": "Add headers to issue threads",
        "user": {
            "login": "tsani",
            "id": 903787,
            "node_id": "MDQ6VXNlcjkwMzc4Nw==",
            "avatar_url": "https://avatars1.githubusercontent.com/u/903787?v=4",
