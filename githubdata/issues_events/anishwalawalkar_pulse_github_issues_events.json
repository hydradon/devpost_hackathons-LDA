                "type": "User",
                "site_admin": false
            },
            "labels": [
                {
                    "id": 262375471,
                    "node_id": "MDU6TGFiZWwyNjIzNzU0NzE=",
                    "url": "https://api.github.com/repos/AnishWalawalkar/Pulse/labels/bug",
                    "name": "bug",
